<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <!-- Background circle -->
  <circle cx="16" cy="16" r="15" fill="#2ecc71" stroke="#27ae60" stroke-width="1"/>
  
  <!-- Wheat/plant icon -->
  <g transform="translate(16, 16)">
    <!-- Main stem -->
    <line x1="0" y1="8" x2="0" y2="-6" stroke="#f39c12" stroke-width="2" stroke-linecap="round"/>
    
    <!-- Left wheat grains -->
    <ellipse cx="-4" cy="-4" rx="2" ry="3" fill="#f1c40f" transform="rotate(-30 -4 -4)"/>
    <ellipse cx="-5" cy="-1" rx="2" ry="3" fill="#f1c40f" transform="rotate(-25 -5 -1)"/>
    <ellipse cx="-5" cy="2" rx="2" ry="3" fill="#f1c40f" transform="rotate(-20 -5 2)"/>
    
    <!-- Right wheat grains -->
    <ellipse cx="4" cy="-4" rx="2" ry="3" fill="#f1c40f" transform="rotate(30 4 -4)"/>
    <ellipse cx="5" cy="-1" rx="2" ry="3" fill="#f1c40f" transform="rotate(25 5 -1)"/>
    <ellipse cx="5" cy="2" rx="2" ry="3" fill="#f1c40f" transform="rotate(20 5 2)"/>
    
    <!-- Center wheat grain -->
    <ellipse cx="0" cy="-6" rx="2.5" ry="3.5" fill="#f39c12"/>
    
    <!-- Leaves -->
    <path d="M-2,0 Q-4,2 -3,4 Q-1,3 0,2" fill="#27ae60" opacity="0.8"/>
    <path d="M2,0 Q4,2 3,4 Q1,3 0,2" fill="#27ae60" opacity="0.8"/>
  </g>
  
  <!-- Small accent dots representing multilingual -->
  <circle cx="8" cy="8" r="1" fill="#3498db" opacity="0.7"/>
  <circle cx="24" cy="8" r="1" fill="#e74c3c" opacity="0.7"/>
  <circle cx="8" cy="24" r="1" fill="#f39c12" opacity="0.7"/>
  <circle cx="24" cy="24" r="1" fill="#9b59b6" opacity="0.7"/>
</svg>
